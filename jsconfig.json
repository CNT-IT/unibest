{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "outDir": "dist",
    "lib": ["esnext", "dom"],
    "types": [
      "@dcloudio/types",
      "@types/wechat-miniprogram",
      "wot-design-uni/global.d.ts",
      "./components.d.ts",
      "./global.d.ts"
    ]
  },
  "vueCompilerOptions": {
    "target": 3,
    "nativeTags": ["block", "template", "component", "slot"]
  },
  "exclude": ["node_modules"],
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/**/*.json",
    "src/env.d.js",
    "src/utils/platform.js",
    "src/utils/index.js",
    "src/utils/http.js",
    "src/store/user.js",
    "src/store/index.js",
    "src/service/foo.js",
    "src/interceptors/route.js",
    "src/interceptors/request.js",
    "src/interceptors/index.js"
  ]
}
